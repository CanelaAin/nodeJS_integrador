<% for (let i = 0; i <= (cantidad -1); i++ ){ %>
<li class="ul__item glide__slide">
  <article>
    <a class="ul__articulos" href="/item/<%= items[i].id %>">
      <picture class="card-item__cover">
        <img class="card-item__img--front" src="<%= items[i].image %>" alt="<%= items[i].alt %>" />
        <img class="card-item__img--back" src="<%= items[i].imageHoover %>" alt="<%= items[i].alt %>" />
      </picture>
      <div class="ul__descripcion">
        <p><%= items[i].brand%></p>
        <h4><%= items[i].title%></h4>
        <p><%= items[i].price%></p>
        <p><%= items[i].payment%></p>
      </div>
      <span class="articulos__sticker"><%= items[i].sticker%></span>
    </a>
  </article>
</li>
<% } %>
